{
  "browser_action": {
    "browser_style": true,
    "default_title": "Camera Viewer",
    "default_icon": {
      "19": "icons/iconSVG.svg",
      "38": "icons/iconSVG.svg"
    }
  },
  "icons": {
    "48": "icons/iconSVG.svg",
    "96": "icons/iconSVG.svg"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "description": "View Your IP Camera/s",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/window-manipulator",
  "manifest_version": 2,
  "name": "Camera Viewer",
  "version": "1.0",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>", "storage"]
}
